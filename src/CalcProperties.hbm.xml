<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="il.aircon.model">
 <class name="CalcProperties" table="calc_properties">
  <id column="uid" name="uid">
   <generator class="native"/>
  </id>
  <property generated="never" lazy="false" name="modulesInstallationCost" type="currency"/>
  <property generated="never" lazy="false" name="baseTubeLength" />
  <property generated="never" lazy="false" name="tubeCost" type="currency"/>
  <property generated="never" lazy="false" name="coolantCost" type="currency"/>
  <property generated="never" lazy="false" name="pumpCost" type="currency"/>

 </class>
</hibernate-mapping>
