<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="il.aircon.model">
 <class name="CalcProperties" table="calc_properties">
  <id column="uid" name="uid" access="field">
   <generator class="native"/>
  </id>
  <property generated="never" lazy="false" name="modulesInstallationCost" type="big_decimal" access="field"/>
  <property generated="never" lazy="false" name="basePipeLength" access="field"/>
  <property generated="never" lazy="false" name="pipeCost" type="big_decimal" access="field"/>
  <property generated="never" lazy="false" name="coolantCost" type="big_decimal" access="field"/>
  <property generated="never" lazy="false" name="pumpCost" type="big_decimal" access="field"/>
  <property generated="never" lazy="false" name="pumpInstallationCost" type="big_decimal" access="field"/>
 </class>
</hibernate-mapping>
